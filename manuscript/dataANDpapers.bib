@Article{Yates2003,
  Title                    = {A NEW SPECIES OF THE PRIMITIVE DINOSAUR THECODONTOSAURUS (SAURISCHIA: SAUROPODOMORPHA) AND ITS IMPLICATIONS FOR THE SYSTEMATICS OF EARLY DINOSAURS},
  Author                   = {Yates, Adam M.},
