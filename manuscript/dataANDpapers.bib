  Author                   = {Zaher, Hussam and Pol, Diego and Carvalho, Alberto B. and Nascimento, Paulo M. and Riccomini, Claudio and Larson, Peter and Juarez-Valieri, Rub\'{e}n and Pires-Domingues, Ricardo and da Silva, Nelson J. and de Almeida Campos, Di\'{o}genes},
