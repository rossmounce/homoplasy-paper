@Article{Wu2001,
  Title                    = {{Terminonaris (Archosauria: Crocodyliformes): new material from Saskatchewan, Canada, and comments on its phylogenetic relationships}},
  Author                   = {Wu, Xiao-Chun and Russell, Anthony P. and Cumbaa, Stephen L.},
  Journal                  = {Journal of Vertebrate Paleontology},
