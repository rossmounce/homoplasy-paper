@Article{Rieppel2002,
  Title                    = {{The skull of the pistosaur Augustasaurus from the Middle Triassic of northwestern Nevada}},
  Author                   = {Rieppel, Olivier and Sander, P. Martin and Storrs, Glenn W.},
