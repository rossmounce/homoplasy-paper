@Article{Pol2003,
  Title                    = {{New remains of Sphagesaurus huenei (Crocodylomorpha: Mesoeucrocodylia) from the Late Cretaceous of Brazil}},
  Author                   = {Pol, Diego},
  Journal                  = {Journal of Vertebrate Paleontology},
