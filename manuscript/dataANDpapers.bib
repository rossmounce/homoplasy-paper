  Citeulike-linkout-1      = {http://www.ingentaconnect.com/content/bpl/pala/1999/00000042/00000005/art00003},
  Citeulike-linkout-2      = {http://www3.interscience.wiley.com/cgi-bin/abstract/119068732/ABSTRACT},
  Doi                      = {10.1111/1475-4983.00098},
  ISSN                     = {0031-0239},
  Keywords                 = {nvmorphophylogeny, peerj},
  Posted-at                = {2014-10-10 14:05:12},
  Priority                 = {2},
  Publisher                = {Wiley-Blackwell},
