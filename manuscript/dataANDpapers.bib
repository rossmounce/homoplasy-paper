  Url                      = {http://dx.doi.org/10.1007/s10914-010-9136-8}
}

@Article{Liu2011,
  Title                    = {{A new pachypleurosaur (Reptilia: Sauropterygia) from the lower Middle Triassic of southwestern China and the phylogenetic relationships of Chinese pachypleurosaurs}},
  Author                   = {Liu, Jun and Rieppel, Olivier and Jiang, Da-Yong and Aitchison, Jonathan C. and Motani, Ryosuke and Zhang, Qi-Yue and Zhou, Chang-Yong and Sun, Yuan-Yuan},
