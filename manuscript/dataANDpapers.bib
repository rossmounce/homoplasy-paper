@Article{Choiniere2010a,
  Title                    = {{A Basal Alvarezsauroid Theropod from the Early Late Jurassic of Xinjiang, China}},
  Author                   = {Choiniere, Jonah N. and Xu, Xing and Clark, James M. and Forster, Catherine A. and Guo, Yu and Han, Fenglu},
