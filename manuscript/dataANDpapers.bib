@Article{Hampe2013,
  Title                    = {{The forgotten remains of a leptocleidid plesiosaur (Sauropterygia: Plesiosauroidea) from the Early Cretaceous of Gronau (M\"{u}nsterland, Westphalia, Germany)}},
