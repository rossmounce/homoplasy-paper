@Article{Hu2009,
  Title                    = {{A pre-Archaeopteryx troodontid theropod from China with long feathers on the metatarsus}},
  Author                   = {Hu, Dongyu and Hou, Lianhai and Zhang, Lijun and Xu, Xing},
  Journal                  = {Nature},
  Year                     = {2009},

  Month                    = sep,
  Number                   = {7264},
  Pages                    = {640--643},
  Volume                   = {461},

  Abstract                 = {{The early evolution of the major groups of derived non-avialan theropods is still not well understood, mainly because of their poor fossil record in the Jurassic. A well-known result of this problem is the 'temporal paradox' argument that is sometimes made against the theropod hypothesis of avian origins1. Here we report on an exceptionally well-preserved small theropod specimen collected from the earliest Late Jurassic Tiaojishan Formation of western Liaoning, China2. The specimen is referable to the Troodontidae, which are among the theropods most closely related to birds. This new find refutes the 'temporal paradox'1 and provides significant information on the temporal framework of theropod divergence. Furthermore, the extensive feathering of this specimen, particularly the attachment of long pennaceous feathers to the pes, sheds new light on the early evolution of feathers and demonstrates the complex distribution of skeletal and integumentary features close to the dinosaur–bird transition.}},
  Citeulike-article-id     = {5881130},
  Citeulike-linkout-0      = {http://dx.doi.org/10.1038/nature08322},
  Citeulike-linkout-1      = {http://dx.doi.org/10.1038/nature08322},
  Day                      = {24},
  Doi                      = {10.1038/nature08322},
  ISSN                     = {0028-0836},
  Keywords                 = {birds, morphophylogeny, peerj},
  Posted-at                = {2010-03-16 17:34:27},
  Priority                 = {2},
  Publisher                = {Nature Publishing Group},
  Url                      = {http://dx.doi.org/10.1038/nature08322}
}

@Article{Hu2011,
