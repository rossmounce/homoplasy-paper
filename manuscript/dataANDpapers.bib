}

@Article{Xu2006,
  Title                    = {{A basal tyrannosauroid dinosaur from the Late Jurassic of China}},
  Author                   = {Xu, Xing and Clark, James M. and Forster, Catherine A. and Norell, Mark A. and Erickson, Gregory M. and Eberth, David A. and Jia, Chengkai and Zhao, Qi},
  Journal                  = {Nature},
  Year                     = {2006},

  Month                    = feb,
  Number                   = {7077},
  Pages                    = {715--718},
  Volume                   = {439},

  Abstract                 = {{The tyrannosauroid fossil record is mainly restricted to Cretaceous sediments of Laurasia, although some very fragmentary Jurassic specimens have been referred to this group1, 2. Here we report a new basal tyrannosauroid, Guanlong wucaii gen. et sp. nov., from the lower Upper Jurassic of the Junggar Basin3, 4, northwestern China. G. wucaii is the oldest known tyrannosauroid and shows several unexpectedly primitive pelvic features5, 6. Nevertheless, the limbs of G. wucaii share several features with derived coelurosaurs7, 8, 9, and it possesses features shared by other coelurosaurian clades10. This unusual combination of character states provides an insight into the poorly known early radiation of the Coelurosauria. Notably, the presumed predatory Guanlong has a large, fragile and highly pneumatic cranial crest that is among the most elaborate known in any non-avian dinosaur and could be comparable to some classical exaggerated ornamental traits among vertebrates.}},
  Citeulike-article-id     = {499630},
  Citeulike-linkout-0      = {http://dx.doi.org/10.1038/nature04511},
  Citeulike-linkout-1      = {http://dx.doi.org/10.1038/nature04511},
