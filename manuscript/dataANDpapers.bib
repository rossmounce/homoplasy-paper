  Keywords                 = {allfossil, class\_reptilia, morphophylogeny, peerj, superorder\_ichthyopterygia},
  Posted-at                = {2011-05-24 13:05:03},
  Priority                 = {2},
  Publisher                = {Springer-Verlag},
  Url                      = {http://dx.doi.org/10.1007/bf02987949}
}

