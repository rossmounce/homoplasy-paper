  Citeulike-linkout-5      = {http://dx.doi.org/10.1666/0022-3360(2003)077\%3C1086:eapron\%3E2.0.co;2},
  Citeulike-linkout-6      = {http://jpaleontol.geoscienceworld.org/cgi/content/abstract/77/6/1086},
  Day                      = {1},
