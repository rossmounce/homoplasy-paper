  Keywords                 = {morphophylogeny, peerj},
  Posted-at                = {2014-10-12 19:43:44},
  Priority                 = {2},
  Publisher                = {Taylor \& Francis},
  Url                      = {http://dx.doi.org/10.1080/02724634.1996.10011358}
}

@Article{Wu1997,
  Title                    = {{Sichuanosuchus shuhanensis, a new ?Early Cretaceous protosuchian (Archosauria: Crocodyliformes) from Sichuan (China), and the monophyly of Protosuchia}},
  Author                   = {Wu, Xiao-Chun and Sues, Hans-Dieter and Dong, Zhi-Ming},
