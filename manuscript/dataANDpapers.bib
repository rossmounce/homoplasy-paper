@Article{Rieppel1994,
  Title                    = {{The braincases of Simosaurus and Nothosaurus: Monophyly of the Nothosauridae (Reptilia: Sauropterygia)}},
  Author                   = {Rieppel, Olivier},
  Journal                  = {Journal of Vertebrate Paleontology},
