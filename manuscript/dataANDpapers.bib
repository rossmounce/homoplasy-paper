@Article{Chinnery1998,
  Title                    = {{Montanoceratops cerorhynchus (Dinosauria: Ceratopsia) and relationships among basal neoceratopsians}},
  Author                   = {Chinnery, Brenda J. and Weishampel, David B.},
  Journal                  = {Journal of Vertebrate Paleontology},
  Year                     = {1998},

  Month                    = sep,
  Number                   = {3},
  Pages                    = {569--585},
  Volume                   = {18},

  Abstract                 = {{ABSTRACT Montanoceratops cerorhynchus has been described as the sister group of Ceratopsidae, even though analyses and diagnosis of this taxon have been tentative and incomplete. A second specimen of M. cerorhynchus includes new diagnostic elements, most notably a partial skull including the caudal half of the braincase, pectoral girdle and manus. Results of a cladistic analysis of eight basal neoceratopsians (Protoceratops, Leptoceratops, Bagaceratops, Microceratops, Breviceratops, Montanoceratops, Asiaceratops, and Udanoceratops) and four ceratopsids (Centrosaurus, Anchiceratops, Chasmosaurus, and Styracosaurus) confirm Montanoceratops as the sister group of Ceratopsidae. Microceratops and Asiaceratops are positioned at the base of Neoceratopsia. Protoceratops, Leptoceratops, and Udanoceratops constitute a monophyletic Protoceratopsidae. Paleogeographical interpretation of the cladogram suggests that Neoceratopsia originated in Asia, and that there were at least two migrations of ceratopsians from Asia to North America.}},
  Citeulike-article-id     = {13387734},
  Citeulike-linkout-0      = {http://dx.doi.org/10.1080/02724634.1998.10011085},
  Citeulike-linkout-1      = {http://www.tandfonline.com/doi/abs/10.1080/02724634.1998.10011085},
