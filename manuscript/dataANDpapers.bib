@Article{Turner2011,
  Title                    = {{Anatomy of Mahakala omnogovae (Theropoda: Dromaeosauridae), T\"{o}gr\"{o}giin Shiree, Mongolia}},
  Author                   = {Turner, Alan H. and Pol, Diego and Norell, Mark A.},
  Journal                  = {American Museum Novitates},
