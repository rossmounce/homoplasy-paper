@Article{Bennett1996,
  Title                    = {{The phylogenetic position of the Pterosauria within the Archosauromorpha}},
  Author                   = {Bennett, S. Christopher},
  Journal                  = {Zoological Journal of the Linnean Society},
  Year                     = {1996},

  Month                    = nov,
  Number                   = {3},
  Pages                    = {261--308},
  Volume                   = {118},

  Abstract                 = {{In recent years the hypothesis that pterosaurs were the major sister-group of dinosaurs and a closely-linked hypothesis that pterosaurs evolved flight from the ground up have gained general acceptance. A cladistic analysis of the Archosauromorpha using characters presented by previous workers results in a single most parsimonious tree with the Pterosauria as the major sister-group of the Dinosauria. However, that sister-group relationship is supported only by a suite of hindlimb characters that are correlated with bipedal digitigrade locomotion in dinosaurs. In pterosaurs the characters have been interpreted as correlates of bipedal cursorial locomotion, arboreal leaping, or involvement of the hindlimb in the wing. The homology of those characters in dinosaurs and pterosaurs cannot be supported. Reanalysis of the data after exclusion of those hindlimb characters results in most parsimonious trees with the Pterosauria as the sister-group of the Erythrosuchidae + Proterochampsidae +Euparkeria + Archosauria, in that order. This sister-group relationship is supported by a diverse assemblage of functionally independent skeletal characters from all regions of the skeleton. The results of the analysis cast doubt on the hypothesis that pterosaurs evolved flight from the ground up.}},
  Citeulike-article-id     = {13386998},
  Citeulike-linkout-0      = {http://dx.doi.org/10.1111/j.1096-3642.1996.tb01267.x},
  Day                      = {1},
  Doi                      = {10.1111/j.1096-3642.1996.tb01267.x},
  Keywords                 = {morphophylogeny, peerj},
  Posted-at                = {2014-10-09 17:58:46},
  Priority                 = {2},
  Publisher                = {Blackwell Publishing Ltd},
  Url                      = {http://dx.doi.org/10.1111/j.1096-3642.1996.tb01267.x}
}

