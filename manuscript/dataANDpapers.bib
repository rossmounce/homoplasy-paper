  ISSN                     = {0024-4082},
  Keywords                 = {allfossil, infraorder\_ornithopoda, jkdino, morphophylogeny, order\_ornithischia, peerj, superfamily\_hadrosauroidea, superorder\_dinosauria, unrepeatable, zoojexport},
