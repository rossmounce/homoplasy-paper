@Article{Qiang1998,
  Title                    = {{Two feathered dinosaurs from northeastern China}},
  Author                   = {Qiang, Ji and Currie, Philip J. and Norell, Mark A. and Shu-An, Ji},
  Journal                  = {Nature},
  Year                     = {1998},

  Month                    = jun,
  Number                   = {6687},
  Pages                    = {753--761},
  Volume                   = {393},

  Abstract                 = {{Current controversy over the origin and early evolution of birds centres on whether or not they are derived from coelurosaurian theropod dinosaurs. Here we describe two theropods from the Upper Jurassic/Lower Cretaceous Chaomidianzi Formation of Liaoning province, China. Although both theropods have feathers, it is likely that neither was able to fly. Phylogenetic analysis indicates that they are both more primitive than the earliest known avialan (bird), Archaeopteryx. These new fossils represent stages in the evolution of birds from feathered, ground-living, bipedal dinosaurs.}},
