@Article{Wenwei2006,
  Title                    = {ONTOGENY AND RELATIONSHIPS OF THE TRILOBITE PSEUDOPETIGURUS PRANTL AND P\v{R}IBYL},
  Author                   = {Wenwei, Yuan and Fortey, Richard A. and Turvey, Samuel T.},
  Journal                  = {Palaeontology},
