@Article{Li2006,
  Title                    = {{An unusual archosaurian from the marine Triassic of China}},
  Author                   = {Li, Chun and Wu, Xiao-chun and Cheng, Yen-nien and Sato, Tamaki and Wang, Liting},
  Journal                  = {Naturwissenschaften},
  Year                     = {2006},
  Number                   = {4},
  Pages                    = {200--206},
  Volume                   = {93},

  Abstract                 = {{A new Triassic archosaurian from China shows a number of aquatic specializations, of which the most striking is the extreme lateral compression of the long tail. Others that may also reflect aquatic adaptations include platelike scapula and coracoid, elongate neck with extremely long and slender ribs, and reduction of osteoderms. In contrast, its pelvic girdle and hind limb have no aquatic modifications. Anatomic features, taphonomy, and local geological data suggest that it may have lived in a coastal–island environment. This lifestyle, convergent with some Jurassic marine crocodyliforms that lived at least 40 million years later and the saltwater species of extant Crocodylus, contradicts with the prevailing view that Triassic archosaurians were restricted to nonmarine ecosystems. Its mosaic anatomy represents a previously unknown ecomorph within primitive archosaurians.}},
  Booktitle                = {Naturwissenschaften},
  Citeulike-article-id     = {13388158},
  Citeulike-linkout-0      = {http://dx.doi.org/10.1007/s00114-006-0097-y},
