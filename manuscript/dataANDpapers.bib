@Article{Tsuihiji2011,
  Title                    = {{Cranial osteology of a juvenile specimen of Tarbosaurus bataar (Theropoda, Tyrannosauridae) from the Nemegt Formation (Upper Cretaceous) of Bugin Tsav, Mongolia}},
  Author                   = {Tsuihiji, Takanobu and Watabe, Mahito and Tsogtbaatar, Khishigjav and Tsubamoto, Takehisa and Barsbold, Rinchen and Suzuki, Shigeru and Lee, Andrew H. and Ridgely, Ryan C. and Kawahara, Yasuhiro and Witmer, Lawrence M.},
  Journal                  = {Journal of Vertebrate Paleontology},
