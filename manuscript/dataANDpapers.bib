  ISSN                     = {1432-1904},
  Keywords                 = {allfossil, infraorder\_ceratosauria, morphophylogeny, peerj, superorder\_dinosauria, year\_2012},
  Pmid                     = {22552426},
  Posted-at                = {2012-05-07 15:46:52},
