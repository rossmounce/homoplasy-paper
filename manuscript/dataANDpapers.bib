@Article{Carballido2011,
  Title                    = {{The osteology of Chubutisaurus insignis del Corro, 1975 (Dinosauria: Neosauropoda) from the 'middle' Cretaceous of central Patagonia, Argentina}},
  Author                   = {Carballido, Jos\'{e} L. and Pol, Diego and Cerda, Ignacio and Salgado, Leonardo},
  Journal                  = {Journal of Vertebrate Paleontology},
  Year                     = {2011},

  Month                    = feb,
  Number                   = {1},
  Pages                    = {93--110},
  Volume                   = {31},

  Abstract                 = {{ABSTRACT Titanosauriform sauropods were one of the most widely distributed groups of dinosaurs during the Cretaceous. In contrast to most derived forms from the Upper Cretaceous, the most basal taxa of the group are poorly known. Thus, studies on these forms are of special interest for understanding the origin and early evolution of Titanosauria. Chubutisaurus insignis del Corro, 1975, is known from postcranial remains found in the Bayo Overo Member of the Cerro Barcino Formation (Chubut Group), which is commonly regarded as Aptian?Cenomanian in age. Phylogenetic analyses that include Chubutisaurus recovered this taxon as the sister group to Titanosauria. Nevertheless, most published studies have not included this taxon, probably due to its brief description and fragmentary remains. Fieldwork conducted in the quarry where the holotype was found yielded new materials that are regarded as part of the same specimen. These new materials, together with additional undescribed, and briefly described elements, originally collected by G. del Corro, are described here. The new information allows the recognition of a more extensive diagnosis of this taxon. The phylogenetic analysis presented here resolves Chubutisaurus as the most basal somphospondylian.}},
  Citeulike-article-id     = {8795709},
  Citeulike-linkout-0      = {http://www.bioone.org/doi/abs/10.1080/02724634.2011.539651},
  Citeulike-linkout-1      = {http://dx.doi.org/10.1080/02724634.2011.539651},
  Citeulike-linkout-2      = {http://www.tandfonline.com/doi/abs/10.1080/02724634.2011.539651},
  Day                      = {8},
  Doi                      = {10.1080/02724634.2011.539651},
  Keywords                 = {allfossil, order\_saurischia, peerj, superorder\_dinosauria},
  Posted-at                = {2011-02-09 23:14:32},
