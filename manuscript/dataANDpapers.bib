@Article{Lauprasert2009,
  Title                    = {{Khoratosuchus jintasakuli gen. et sp. nov., an advanced neosuchian crocodyliform from the Early Cretaceous (Aptian-Albian) of NE Thailand}},
  Author                   = {Lauprasert, K. and Cuny, G. and Thirakhupt, K. and Suteethorn, V.},
