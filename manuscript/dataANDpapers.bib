  ISSN                     = {00310239},
  Keywords                 = {allfossil, infraorder\_sauropoda, morphophylogeny, peerj, suborder\_sauropodomorpha, superorder\_dinosauria, year\_2012},
  Posted-at                = {2012-03-13 17:53:46},
