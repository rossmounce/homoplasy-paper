  Day                      = {1},
  Doi                      = {10.1666/0022-3360(2001)075\%3C0827:sostwn\%3E2.0.co;2},
  Keywords                 = {nvmorphophylogeny, peerj},
  Posted-at                = {2014-10-12 19:23:31},
  Priority                 = {2},
  Publisher                = {The Paleontological Society},
  Url                      = {http://dx.doi.org/10.1666/0022-3360(2001)075\%3C0827:sostwn\%3E2.0.co;2}
}

