  ISSN                     = {2041-4927},
  Keywords                 = {morphophylogeny, peerj},
  Posted-at                = {2014-10-12 19:08:35},
  Priority                 = {2},
  Publisher                = {Geological Society of London},
  Url                      = {http://dx.doi.org/10.1144/gsl.sp.2003.217.01.11}
}

